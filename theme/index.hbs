                </div>
            </div>
            <div class="page-inner">
                <nav id="sidebar" class="sidebar" aria-label="Table of contents">
                    <div class="sidebar-inner">
                        <span class="sidebar-title">Documentation</span>
                        {{#toc}}{{/toc}}
                        <div class="flex-spacer"></div>
                    </div>
                </nav>
                <div class="page">
                    {{> header}}
                    <!-- Apply ARIA attributes after the sidebar and the sidebar toggle button are added to the DOM -->
                    <script>
                        document.getElementById('sidebar-toggle').setAttribute('aria-expanded', sidebar === 'visible');
                        document.getElementById('sidebar').setAttribute('aria-hidden', sidebar !== 'visible');
                        Array.from(document.querySelectorAll('#sidebar a')).forEach(function (link) {
                            link.setAttribute('tabIndex', sidebar === 'visible' ? 0 : -1);
                        });
                    </script>

                    <div id="content" class="content">
                        <main>
                            {{{ content }}}
                        </main>
                    </div>
                </div>
            </div>
        </div>

        {{#if search_enabled}}
        <input type="checkbox" id="search-toggle-anchor" class="hidden">
        <label class="search-overlay" for="search-toggle-anchor">
            <div class="search-overlay-inner" id="search-overlay-inner">
                <div class="search-input-area">
                    <form id="searchbar-outer" class="searchbar-outer">
                        <svg xmlns="http://www.w3.org/2000/svg" viewBox="0 -960 960 960" fill="currentColor">
                            <path
                                d="M380-320q-109 0-184.5-75.5T120-580q0-109 75.5-184.5T380-840q109 0 184.5 75.5T640-580q0 44-14 83t-38 69l224 224q11 11 11 28t-11 28q-11 11-28 11t-28-11L532-372q-30 24-69 38t-83 14Zm0-80q75 0 127.5-52.5T560-580q0-75-52.5-127.5T380-760q-75 0-127.5 52.5T200-580q0 75 52.5 127.5T380-400Z" />
                        </svg>
                        <input type="search" id="searchbar" name="searchbar" placeholder="Search whisky docs..."
                            aria-controls="searchresults-outer" aria-describedby="searchresults-header">
                    </form>
                    <label for="search-toggle-anchor" class="search-overlay-close" title="Close search overlay" id="search-overlay-close">
                        <svg xmlns="http://www.w3.org/2000/svg" viewBox="0 0 24 24" fill="currentColor">
                            <path
                                d="M18.3 5.71c-.39-.39-1.02-.39-1.41 0L12 10.59 7.11 5.7c-.39-.39-1.02-.39-1.41 0-.39.39-.39 1.02 0 1.41L10.59 12 5.7 16.89c-.39.39-.39 1.02 0 1.41.39.39 1.02.39 1.41 0L12 13.41l4.89 4.89c.39.39 1.02.39 1.41 0 .39-.39.39-1.02 0-1.41L13.41 12l4.89-4.89c.38-.38.38-1.02 0-1.4z" />
                        </svg>
                    </label>
                </div>
                <div id="searchresults-outer" class="searchresults-outer">
                    <div id="searchresults-header"></div>
                    <ul id="searchresults">
                    </ul>
                </div>
            </div>
        </label>
        {{/if}}

        <!-- Hide / unhide sidebar before it is displayed -->
        <script>
            var body = document.querySelector('body');
            var sidebarToggle = document.getElementById('sidebar-toggle-anchor');

            if (document.body.clientWidth >= 1080) {
                var sidebar_target_state = false
                try { sidebar_target_state = localStorage.getItem('mdbook-sidebar') === 'visible' } catch (e) { }
                sidebarToggle.checked = sidebar_target_state;
            } else {
                sidebarToggle.checked = false;
            }

            setTimeout(function () {
                document.body.classList.add('sidebar-loaded');
            }, 0);
        </script>

        <!-- Track and set sidebar scroll position -->
        <script defer>
            var sidebarScrollbox = document.getElementById('sidebar').children[0];
            sidebarScrollbox.addEventListener('click', function (e) {
                if (e.target.tagName === 'A') {
                    sessionStorage.setItem('sidebar-scroll', sidebarScrollbox.scrollTop);
                }
            });
            var sidebarScrollTop = sessionStorage.getItem('sidebar-scroll');
            sessionStorage.removeItem('sidebar-scroll');
            if (sidebarScrollTop) {
                // preserve sidebar scroll position when navigating via links within sidebar
                // sidebarScrollbox.scrollTop = Math.min(sidebarScrollTop, sidebarScrollbox.scrollHeight - sidebarScrollbox.clientHeight);
            } else {
                // scroll sidebar to current active section when navigating via "next/previous chapter" buttons
                var activeSection = document.querySelector('#sidebar .active');
                if (activeSection) {
                    // Go to old active section
                    activeSection.scrollIntoView({ block: 'center', behavior: 'smooth' });
                }
            }
        </script>

        <!-- Sidebar toggle -->
        <script>
            var sidebarToggle = document.getElementById('sidebar-toggle-anchor');

            sidebarToggle.addEventListener('input', function () {
                var sidebarState = sidebarToggle.checked;

                try {
                    localStorage.setItem('mdbook-sidebar', sidebarState ? 'visible' : 'hidden');
                } catch (e) { }
            });
        </script>

        {{#if search_enabled}}
        <!-- Keybinds for search -->
        <script>
            var searchToggle = document.getElementById('search-toggle-anchor');
            var searchShortcut = document.getElementById('search-shortcut');
            var searchOutter = document.getElementById('search-overlay-inner');
            var searchCloser = document.getElementById('search-overlay-close');

            // On macos the key is 'Meta' instead of 'Control'
            var isMac = /(Mac|iPhone|iPod|iPad)/i.test(navigator.platform);

            // Update the search shortcut text to reflect the correct key
            if (isMac) {
                searchShortcut.textContent = 'âŒ˜ K';
            } else {
                searchShortcut.textContent = 'Ctrl K';
            }

            document.addEventListener('keydown', function (event) {
                console.log(event.key);
                if (event.key === 'k' && (isMac ? event.metaKey : event.ctrlKey)) {
                    searchToggle.checked = !searchToggle.checked;
                    document.getElementById('searchbar').focus();
                    event.preventDefault();
                } else if (event.key === 'Escape' && searchToggle.checked) {
                    searchToggle.checked = false;
                    event.preventDefault();
                }
            });

            searchOutter.addEventListener('click', function (event) {
                event.preventDefault();
                if (event.target === searchOutter || event.target === searchCloser) {
                    searchToggle.checked = false;
                } else if (event.target.tagName === 'A') {
                    // Open the link
                    location.href = event.target.href;
                }
            });
        </script>

        <!-- Searchbar virtual selection -->
        <script>
            var searchbar = document.getElementById('searchbar');
            var searchresults = document.getElementById('searchresults');
            var searchresultsHref = null;

            function compareUrlPathWithoutSearch(a, b) {
                var urlA = new URL(a);
                var urlB = new URL(b);
                return urlA.pathname === urlB.pathname && urlA.hash === urlB.hash;
            }

            function findIndexFromHref(href) {
                var childrenElements = Array.from(searchresults.children);
                for (var i = 0; i < childrenElements.length; i++) {
                    var item = childrenElements[i];
                    var a = item.children[0];
                    if (compareUrlPathWithoutSearch(a.href, href)) {
                        return i;
                    }
                }
                return -1;
            }

            searchbar.addEventListener('keydown', function (event) {
                if (([
                    'ArrowUp',
                    'ArrowDown',
                    'Enter'
                ].indexOf(event.key) === -1)) {
                    return;
                }

                event.preventDefault();
                event.stopPropagation();

                if (searchresults.children.length === 0) {
                    return;
                }

                var searchresultsIndex = -1;

                if (searchresultsHref !== null) {
                    searchresultsIndex = findIndexFromHref(searchresultsHref);
                }

                if (event.key === 'ArrowUp') {
                    searchresultsIndex = Math.max(0, searchresultsIndex - 1);
                } else if (event.key === 'ArrowDown') {
                    searchresultsIndex = Math.min(searchresults.children.length - 1, searchresultsIndex + 1);
                } else if (event.key === 'Enter') {
                    if (searchresultsIndex !== -1) {
                        window.location.href = searchresults.children[searchresultsIndex].children[0].href;
                    }
                    return;
                }

                // Add the active class to the selected item
                searchresultsHref = searchresults.children[searchresultsIndex].children[0].href;
                for (var i = 0; i < searchresults.children.length; i++) {
                    searchresults.children[i].classList.remove('active');
                }
                searchresults.children[searchresultsIndex].classList.add('active');

                // Scroll the selected item into view
                searchresults.children[searchresultsIndex].scrollIntoView({
                    block: 'nearest'
                });
            });

            document

            // When the searchresults are updated
            var observer = new MutationObserver(function (mutations) {
                if (searchresults.children.length === 0) {
                    searchresultsHref = null;
                    return;
                }

                // Check if the href still exists in the search results
                var searchresultsIndex = -1;
                if (searchresultsHref !== null) {
                    searchresultsIndex = findIndexFromHref(searchresultsHref);
                }

                // If it does, add the active class and remove it from the others
                if (searchresultsIndex !== -1) {
                    for (var i = 0; i < searchresults.children.length; i++) {
                        searchresults.children[i].classList.remove('active');
                    }
                    searchresults.children[searchresultsIndex].classList.add('active');
                    searchresultsHref = searchresults.children[searchresultsIndex].children[0].href;
                    searchresults.children[searchresultsIndex].scrollIntoView({
                        block: 'nearest'
                    });
                } else {
                    // Select the first item if it exists
                    if (searchresults.children.length > 0) {
                        for (var i = 0; i < searchresults.children.length; i++) {
                            searchresults.children[i].classList.remove('active');
                        }
                        searchresults.children[0].classList.add('active');
                        searchresultsHref = searchresults.children[0].children[0].href;
                        searchresults.children[0].scrollIntoView({
                            block: 'nearest'
                        });
                    } else {
                        searchresultsHref = null;
                    }
                }
            });

            observer.observe(searchresults, {
                childList: true
            });
        </script>
        {{/if}}

        {{#if live_reload_endpoint}}
        <!-- Livereload script (if served using the cli tool) -->
        <script>
            const wsProtocol = location.protocol === 'https:' ? 'wss:' : 'ws:';
            const wsAddress = wsProtocol + "//" + location.host + "/" + "{{{live_reload_endpoint}}}";
            const socket = new WebSocket(wsAddress);
            socket.onmessage = function (event) {
                if (event.data === "reload") {
                    socket.close();
                    location.reload();
                }
            };

            window.onbeforeunload = function () {
                socket.close();
            }
        </script>
        {{/if}}

        {{#if playground_line_numbers}}
        <script>
            window.playground_line_numbers = true;
        </script>
        {{/if}}

        {{#if playground_copyable}}
        <script>
            window.playground_copyable = true;
        </script>
        {{/if}}

        {{#if playground_js}}
        <script src="{{ path_to_root }}ace.js"></script>
        <script src="{{ path_to_root }}editor.js"></script>
        <script src="{{ path_to_root }}mode-rust.js"></script>
        <script src="{{ path_to_root }}theme-dawn.js"></script>
        <script src="{{ path_to_root }}theme-tomorrow_night.js"></script>
        {{/if}}

        {{#if search_js}}
        <script src="{{ path_to_root }}elasticlunr.min.js"></script>
        <script src="{{ path_to_root }}mark.min.js"></script>
        <script src="{{ path_to_root }}searcher.js"></script>
        {{/if}}

        <script src="{{ path_to_root }}clipboard.min.js"></script>
        <script src="{{ path_to_root }}highlight.js"></script>
        <script src="{{ path_to_root }}book.js"></script>

        <!-- Custom JS scripts -->
        {{#each additional_js}}
        <script src="{{ ../path_to_root }}{{this}}"></script>
        {{/each}}

        {{#if is_print}}
        {{#if mathjax_support}}
        <script>
            window.addEventListener('load', function () {
                MathJax.Hub.Register.StartupHook('End', function () {
                    window.setTimeout(window.print, 100);
                });
            });
        </script>
        {{else}}
        <script>
            window.addEventListener('load', function () {
                window.setTimeout(window.print, 100);
            });
        </script>
        {{/if}}
        {{/if}}

    </div>
</body>

</html>