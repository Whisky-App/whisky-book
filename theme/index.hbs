                    {{/if}}
                    <div class="menu-bar-divider"></div>
                    {{#if search_enabled}}
                    <label class="menu-bar-button" for="search-toggle-anchor" title="Search" aria-label="Search">
                        <svg xmlns="http://www.w3.org/2000/svg" viewBox="0 -960 960 960" fill="currentColor">
